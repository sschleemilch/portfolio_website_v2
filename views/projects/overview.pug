  - var projects_active=true

block projects_content
  div(class="section")
    div(class="container")
      h1(class="title")
        strong Projects Overview
      div(class="content has-text-centered")
        div(class="lb is-primary is-projects")
          div(class="columns")
            a(class="column" href="/projects/gitbash")
              img(src="/images/git_128x128.png" alt="Git", width="42", height="42")
              p(class="title is-4")
                strong Git Bash
              p(class="subtitle") A custom Git Bash Prompt
    